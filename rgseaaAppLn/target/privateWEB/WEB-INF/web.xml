<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         id="WebApp_ID" version="3.0">

    <env-entry>
        <env-entry-name>PATH_REPO</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>C:/repo/wld-des-206/apps/rgseaa-web-jee-r01a-id</env-entry-value>
    </env-entry>

    <!-- App -->
    <env-entry>
        <env-entry-name>ACTIVE_PROFILE</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>local</env-entry-value>
    </env-entry>

    <env-entry>
        <env-entry-name>CONTEXT_PATH</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>@context.path@</env-entry-value>
    </env-entry>
    <!-- / App -->

    <!-- Database resources -->
    <env-entry>
        <env-entry-name>DB_JNDI_NAME</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>not-used</env-entry-value>
    </env-entry>
    <env-entry>
        <env-entry-name>DB_SCHEMA_NAME</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>WEB_BD_A_O</env-entry-value>
    </env-entry>
    <!-- / Database resources -->


</web-app>
